{
  "Collect": {
    "Toolset": [
      {
        "name": "nmap",
        "commonTags": "nginx, iis",
        "frameworkTags": "joomla, wordpress",
        "hostTags": "windows, linux, ubuntu, centos",
        "serverTags": "windows, linux, ubuntu, centos",
        "info": "nmap",
        "commandLine": "nmap --open -F {0}",
        "versionCommandLine" : "nmap -V",
        "timeout": "10000"
      }
    ]
  },
  "Scan": {
    "Toolset": [
      {
        "name": "nmap",
        "commonTags": "nginx, iis",
        "frameworkTags": "joomla, wordpress",
        "hostTags": "windows, linux, ubuntu, centos",
        "serverTags": "windows, linux, ubuntu, centos",
        "info": "nmap",
        "commandLine": "nmap --script nmap-vulners -sV -Pn {0}",
        "versionCommandLine" : "nmap -V",
        "timeout": "10000"
      }
    ]
  }
}